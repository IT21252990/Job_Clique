        app:layout_constraintTop_toTopOf="@+id/materialToolbar3" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="227dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:text="@string/wishlist_itemhead3"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/materialToolbar4" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:text="@string/wishlist_subhead1"
        android:textColor="@color/material_dynamic_neutral40"
        android:textSize="@dimen/wishlist_subhead_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/materialToolbar"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.285" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:text="@string/wishlist_subhead"
        android:textColor="@color/material_dynamic_neutral40"
        android:textSize="@dimen/wishlist_subhead_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/materialToolbar5"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.259" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:text="@string/wishlist_subhead4"
        android:textColor="@color/material_dynamic_neutral40"
        android:textSize="@dimen/wishlist_subhead_text_size"
        app:layout_constraintBottom_toBottomOf="@+id/materialToolbar3"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
